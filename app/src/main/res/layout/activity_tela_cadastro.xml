<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TelaCadastroActivity">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="421dp"
            android:layout_height="928dp"
            android:alpha="0.6"
            android:background="@drawable/back"
            android:contentDescription="@string/todo"
            app:srcCompat="@drawable/imagem_odonto" />


    </LinearLayout>

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageView5"
        android:layout_width="200dp"
        android:layout_height="198dp"
        android:layout_marginTop="16dp"
        android:padding="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:shapeAppearance="@style/ShapeAppearance.Material3.Tooltip"
        app:shapeAppearanceOverlay="@style/circular"
        app:srcCompat="@drawable/female"
        app:strokeColor="@color/grean3"
        app:strokeWidth="5dp"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Bem vindo!"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="O seu app de \n atendimento facil"
        android:textAlignment="center"
        android:textColor="@color/grean_oh"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="320dp"
        android:layout_weight="1"
        android:background="@drawable/__icon__arrow_back_circle_sharp_"
        android:onClick="mainactivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout" />

    <EditText
        android:id="@+id/editTextUsername"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/sla"
        android:textCursorDrawable="@color/grean3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <EditText
        android:id="@+id/editTextEmail"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_below="@id/editTextUsername"
        android:layout_marginTop="32dp"
        android:background="@drawable/sla"
        android:inputType="textEmailAddress"
        android:padding="15dp"
        android:textCursorDrawable="@color/grean3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextUsername" />

    <EditText
        android:id="@+id/editTextCRO"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_below="@id/editTextUsername"
        android:layout_marginTop="32dp"
        android:background="@drawable/sla"
        android:inputType="text"
        android:padding="15dp"
        android:textCursorDrawable="@color/grean3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextEmail" />

    <EditText
        android:id="@+id/editTextPassword"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_below="@id/editTextUsername"
        android:layout_marginTop="32dp"
        android:background="@drawable/sla"

        android:inputType="textPassword"
        android:padding="15dp"
        android:textCursorDrawable="@color/grean3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextCRO" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_below="@id/editTextUsername"
        android:layout_marginTop="32dp"

        android:background="@drawable/sla"
        android:inputType="text|phone"
        android:padding="15dp"
        android:textCursorDrawable="@color/grean3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPassword" />

    <Button
        android:id="@+id/buttonLogin"
        android:layout_width="194dp"
        android:layout_height="45dp"
        android:layout_below="@id/editTextPassword"
        android:layout_marginTop="32dp"
        android:background="@drawable/wfwefwef"
        android:fontFamily="@font/montserrat"
        android:onClick="menuacti"
        android:text="Cadastrar"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textColorHighlight="#673AB7"
        android:textColorLink="#673AB7"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menuEstado" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="250dp"
        android:layout_marginBottom="4dp"
        android:layout_weight="1"
        android:fontFamily="@font/montserrat_bold"
        android:text="Nome"
        android:textColor="@color/grean2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextUsername"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="250dp"
        android:layout_marginBottom="4dp"
        android:layout_weight="1"
        android:fontFamily="@font/montserrat_bold"
        android:text="Email"
        android:textColor="@color/grean2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="190dp"
        android:layout_marginBottom="4dp"
        android:layout_weight="1"
        android:fontFamily="@font/montserrat_bold"
        android:text="CRO/MatrÃ­cula"
        android:textColor="@color/grean2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextCRO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="4dp"
        android:layout_weight="1"
        android:fontFamily="@font/montserrat_bold"
        android:text="Telefone"
        android:textColor="@color/grean2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextPhone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:layout_weight="1"
        android:fontFamily="@font/montserrat_bold"
        android:text="Estado"
        android:textColor="@color/grean2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/menuEstado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/menuEstado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:maxHeight="100dp"
        android:theme="@style/SpinnerPerson"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout"
        app:layout_constraintStart_toStartOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="250dp"
        android:layout_weight="1"
        android:fontFamily="@font/montserrat_bold"
        android:text="Senha"
        android:textColor="@color/grean2"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>