<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".conta">


        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="421dp"
                    android:layout_height="928dp"
                    android:alpha="0.0"
                    android:background="@drawable/back"
                    android:contentDescription="@string/todo"
                    app:srcCompat="@drawable/imagem_odonto" />

                <Button
                    android:id="@+id/button8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Button" />


        </LinearLayout>

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView5"
            android:layout_width="200dp"
            android:layout_height="198dp"
            android:layout_marginBottom="32dp"
            android:padding="9dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextUsername"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:shapeAppearance="@style/ShapeAppearance.Material3.Tooltip"
            app:shapeAppearanceOverlay="@style/circular"
            app:srcCompat="@drawable/female"
            app:strokeColor="@color/grean_oh"
            app:strokeWidth="5dp"
            tools:ignore="NotSibling" />

        <Button
            android:id="@+id/button6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="320dp"
            android:layout_weight="1"
            android:onClick="voltarmenu"
            android:background="@drawable/__icon__arrow_back_circle_sharp_"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/linearLayout" />

        <EditText
            android:id="@+id/editTextUsername"
            android:layout_width="300dp"
            android:layout_height="55dp"
            android:layout_marginBottom="32dp"
            android:background="@drawable/sla"
            android:textCursorDrawable="@color/grean3"
            app:layout_constraintBottom_toTopOf="@+id/editTextEmail"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editTextEmail"
            android:layout_width="300dp"
            android:layout_height="55dp"
            android:layout_below="@id/editTextUsername"
            android:layout_marginBottom="32dp"
            android:background="@drawable/sla"
            android:inputType="textPassword"
            android:padding="15dp"
            android:textCursorDrawable="@color/grean3"
            app:layout_constraintBottom_toTopOf="@+id/editTextCRO"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editTextCRO"
            android:layout_width="300dp"
            android:layout_height="55dp"
            android:layout_below="@id/editTextUsername"
            android:layout_marginTop="619dp"
            android:background="@drawable/sla"
            android:inputType="textPassword"
            android:padding="15dp"
            android:textCursorDrawable="@color/grean3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/linearLayout" />

        <Button
            android:id="@+id/buttonLogin"
            android:layout_width="194dp"
            android:layout_height="45dp"
            android:layout_marginBottom="60dp"
            android:background="@drawable/wfwefwef"
            android:fontFamily="@font/montserrat"
            android:text="Salvar"
            android:textAllCaps="false"
            android:onClick="menuacti"
            android:textColor="@color/white"
            android:textColorHighlight="#673AB7"
            android:textColorLink="#673AB7"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.508"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/buttonns"
            android:layout_width="144dp"
            android:layout_height="45dp"
            android:background="@drawable/button"
            android:fontFamily="@font/montserrat"
            android:onClick="menuacti"
            android:text="Nova Senha"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textColorHighlight="#673AB7"
            android:textColorLink="#673AB7"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/buttonLogin"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.508"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextCRO" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="250dp"
            android:layout_marginBottom="4dp"
            android:layout_weight="1"
            android:fontFamily="@font/montserrat_bold"
            android:text="Nome"
            android:textColor="@color/grean2"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/editTextUsername"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="250dp"
            android:layout_marginBottom="4dp"
            android:layout_weight="1"
            android:fontFamily="@font/montserrat_bold"
            android:text="Email"
            android:textColor="@color/grean2"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/editTextEmail"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="190dp"
            android:layout_marginBottom="4dp"
            android:layout_weight="1"
            android:fontFamily="@font/montserrat_bold"
            android:text="Telefone            "
            android:textColor="@color/grean2"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/editTextCRO"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>